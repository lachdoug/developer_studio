<% consumer_param.input.collection.items.each do |item| %>
  <% if item.index > 0 %>
    $("#<%= "service_blueprint_consumer_param_#{consumer_param.index}_input_collection_item_#{item.index}_moveup_button_disabled" %>").hide();
    $("#<%= "service_blueprint_consumer_param_#{consumer_param.index}_input_collection_item_#{item.index}_moveup_button" %>").show();
  <% end %>
<% end %>
